<% if params[:user_id] %>
  $("#role_form_<%= params[:user_id] %>").html('<%= escape_javascript(render('role_form')) %>')
<% else %>
  $('#enroled').html('<%= escape_javascript(render('enroled')) %>')
  $('#enrole').html('<%= escape_javascript(render('enrole')) %>')
  WEBY.initEnrolePage();
<% end %>
