<% if component.extended_accessibility? %>
  <%= render partial: "menu_accessibility/views/extended", locals: {component: component} %>
<% else %>
  <%= render partial: "menu_accessibility/views/basic", locals: {component: component} %>
<% end %>
<% include_component_javascript :javascripts, 'show' %>
<% include_component_javascript :javascripts, 'changeContrast' %>
