<%= f.input :institution, collection: component.institutions_as_options,
  priority: component.institution,
  include_blank: false
%>
<%= f.input :format, collection: component.format_list.map{|format| [t(".#{format}"), format]},
  priority: component.format,
  include_blank: false
%>
<%= f.input :new_tab, wrapper: :checkbox, as: :boolean %>
<%= f.input :html_class %>